            <DelegateInArgument x:TypeArguments="sx:XmlNode" Name="EmptyCatchNode" />
          </ActivityAction.Argument>
          <Sequence DisplayName="Body" sap2010:WorkflowViewState.IdRef="Sequence_4">
            <Sequence.Variables>
              <Variable x:TypeArguments="x:String" Name="InternalPath" />
              <Variable x:TypeArguments="x:String" Name="TargetName" />
              <Variable x:TypeArguments="sx:XmlNode" Name="IssueTryCatchNode" />
            </Sequence.Variables>
            <Assign sap2010:Annotation.AnnotationText="For a more informative result, sets the issue's target to be the TryCatch activity instead of the Catch block itself, since all Catch blocks have the same name that cannot be changed." DisplayName="Assign IssueTryCatch" sap2010:WorkflowViewState.IdRef="Assign_4">
