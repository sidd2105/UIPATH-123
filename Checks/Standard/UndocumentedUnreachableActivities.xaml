            <If Condition="[UnreachableNode.Attributes(&quot;sap2010:Annotation.AnnotationText&quot;) Is Nothing]" DisplayName="If unreachable node does not have an annotation" sap2010:WorkflowViewState.IdRef="If_1">
              <If.Then>
