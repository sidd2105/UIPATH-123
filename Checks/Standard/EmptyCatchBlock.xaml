              </ui:InvokeWorkflowFile.Arguments>
            </ui:InvokeWorkflowFile>
            <If sap2010:Annotation.AnnotationText="Retrieves the target's display name, if it is defined." Condition="[IssueTryCatchNode.Attributes(&quot;DisplayName&quot;) Is Nothing]" DisplayName="If activity has DisplayName" sap2010:WorkflowViewState.IdRef="If_1">
              <If.Then>
