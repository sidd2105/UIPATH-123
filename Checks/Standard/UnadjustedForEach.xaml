    <ui:ForEach x:TypeArguments="sx:XmlNode" DisplayName="For each node" sap2010:WorkflowViewState.IdRef="ForEach`1_5" Values="[in_XMLTree.DocumentElement.SelectNodes(XPathExpression, in_NamespaceManager)]">
      <ui:ForEach.Body>
        <ActivityAction x:TypeArguments="sx:XmlNode">
